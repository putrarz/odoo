<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_product_items" model="ir.cron">
        <field name="name">Product Scheduler</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_product_product"/>
        <field name="code">model._sync_master_from_accpacc("100","0")</field>
        <field name="state">code</field>
    </record>

    <record id="ir_cron_product_qty_on_hand" model="ir.cron">
        <field name="name">Product Qty on Hand Scheduler</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_product_product"/>
        <field name="code">model._sync_qtyhand_item_from_accpacc("100", "0")</field>
        <field name="state">code</field>
    </record>
</odoo>
